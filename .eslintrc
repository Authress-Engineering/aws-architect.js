{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "cimpress-atsquad"
  ],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "indent": ["error", "tab", { "SwitchCase": 1, "MemberExpression": 0 }],
    "node/no-unsupported-features": ["off"],
    "no-throw-literal": "off",
    "spaced-comment": "off",
    "no-unused-expressions": "off"
  }
}
